{
  "name": "jagesaurus",
  "version": "1.0.0",
  "description": "Jagesaurus - Web-based application for school administrators to create, manage, and distribute exam timetables with automated invigilation scheduling",
  "private": true,
  "homepage": "https://sanjayjagessur.github.io/examfinal",
  "dependencies": {
    "@types/node": "^16.18.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "^10.4.14",
    "date-fns": "^2.30.0",
    "docx": "^9.5.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.0",
    "lucide-react": "^0.263.1",
    "openai": "^5.18.1",
    "papaparse": "^5.4.1",
    "postcss": "^8.4.24",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^4.9.0",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "main": "build/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "npm run build && electron build/electron.js",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron public/electron.js\"",
    "electron-pack": "npm run build && electron-builder",
    "postbuild": "copy public\\electron.js build\\ && copy public\\preload.js build\\",
    "dist": "npm run build && electron-builder",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/papaparse": "^5.3.16",
    "@types/react-beautiful-dnd": "^13.1.4",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "electron": "^37.4.0",
    "electron-builder": "^26.0.12",
    "electron-is-dev": "^3.0.1",
    "gh-pages": "^6.3.0",
    "wait-on": "^8.0.4"
  },
  "build": {
    "appId": "com.jagesaurus.examtimetable",
    "productName": "Jagesaurus",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "target": "portable"
    },
    "portable": {
      "artifactName": "Jagesaurus.exe"
    }
  }
}
